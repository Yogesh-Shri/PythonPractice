n , m = map(int,input().split())

for j in range(1,(n//2)+1):
    print("-"*((m-3*((2*j)-1))//2),".|."*(2*j - 1),"-"*((m-3*((2*j)-1))//2),sep="")
print("-"*((m-7)//2),"WELCOME","-"*((m-7)//2),sep="")
for j in range((n//2),0,-1):
    print("-"*((m-3*((2*j)-1))//2),".|."*(2*j - 1),"-"*((m-3*((2*j)-1))//2),sep="")
